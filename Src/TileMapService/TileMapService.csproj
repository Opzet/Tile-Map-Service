<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFrameworks>net5.0</TargetFrameworks>
		<Nullable>enable</Nullable>
		<UserSecretsId>f052d8ac-cdc5-4dfb-86b5-763554895d34</UserSecretsId>
		<AssemblyName>tms</AssemblyName>
		<RootNamespace>TileMapService</RootNamespace>
	</PropertyGroup>

	<PropertyGroup Condition="'$(VisualStudioVersion)' &gt;= '17.0'">
		<TargetFrameworks>net5.0;net8.0</TargetFrameworks>
	</PropertyGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'net5.0'">
		<PackageReference Include="BitMiracle.LibTiff.NET" Version="2.4.649" />
		<PackageReference Include="Microsoft.Data.Sqlite.Core" Version="7.0.13" />
		<PackageReference Include="Npgsql" Version="6.0.13" />
		<PackageReference Include="SkiaSharp" Version="2.88.6" />
		<PackageReference Include="SkiaSharp.NativeAssets.Linux" Version="2.88.6" />
		<PackageReference Include="SQLitePCLRaw.bundle_green" Version="2.1.7" />
	</ItemGroup>

	<ItemGroup Condition="'$(TargetFramework)' == 'net8.0'">
		<PackageReference Include="BitMiracle.LibTiff.NET" Version="2.4.660" />
		<PackageReference Include="Microsoft.Data.Sqlite.Core" Version="9.0.5" />
		<PackageReference Include="Npgsql" Version="9.0.3" />
		<PackageReference Include="SkiaSharp" Version="3.119.0" />
		<PackageReference Include="SkiaSharp.NativeAssets.Linux.NoDependencies" Version="3.119.0" />
		<PackageReference Include="SQLitePCLRaw.bundle_green" Version="2.1.11" />
	</ItemGroup>

</Project>
